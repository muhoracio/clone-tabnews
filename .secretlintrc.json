{
  "rules": [
    {
      "id": "@secretlint/secretlint-rule-preset-recommend"
    },
    {
      "id": "@secretlint/secretlint-rule-pattern",
      "options": {
        "patterns": [
          {
            "name": "credentials",
            "patterns": [
              "BEGIN RSA PRIVATE KEY",
              "eyJhbGciOi",
              "(?i)secret",
              "(?i)token",
              "private_key"
            ]
          }
        ]
      }
    }
  ]
}
